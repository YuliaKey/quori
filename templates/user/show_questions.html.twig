{% extends "base.html.twig" %}

{% block title %}Mes questions{% endblock %}

{% block body %}
    <div class="block mb-20">
        <h3 class="pb-30">
            Mes questions
        </h3>
        <ul>
            {% for question in app.user.questions %}
                <a href="{{path('show_question', {id: question.id})}}">
                    <li class="pb-30">{{question.title}}</li>
                </a>
            {% endfor %}
        </ul>
    </div>
{% endblock %}